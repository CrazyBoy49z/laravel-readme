{
    "name": "diviky/laravel-readme",
    "description": "Framework extension",
    "keywords": ["laravel", "readme", "markdown", "security"],
    "homepage": "https://github.com/sankarsuda/laravel-readme",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar.suda@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "league/commonmark": "^0.18",
        "webuni/commonmark-table-extension": "^0.9",
        "jonnybarnes/commonmark-linkify": "^0.5",
        "league/commonmark-extras": "^0.2",
        "uafrica/commonmark-ext": "^0.3",
        "webuni/commonmark-attributes-extension": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Diviky\\Readme\\": "src"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Diviky\\Readme\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": ["Diviky\\ReadmeServiceProvider"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
